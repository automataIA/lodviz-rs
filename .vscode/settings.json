{
    "rust-analyzer.rustfmt.overrideCommand": [
        "leptosfmt",
        "--stdin",
        "--rustfmt"
    ],
    "editor.formatOnSave": true,
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
    },
    "files.associations": {
        "*.rs": "rust",
        "*.css": "tailwindcss"
    },
    "css.lint.unknownAtRules": "ignore"
}